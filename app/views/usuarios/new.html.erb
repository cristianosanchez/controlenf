<h1>New usuario</h1>

<%= error_messages_for :usuario %>

<% form_for(@usuario) do |f| %>
  <p>
    <b>Nome</b><br />
    <%= f.text_field :nome %>
  </p>

  <p>
    <b>Email</b><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <b>Senha</b><br />
    <%= f.text_field :senha %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', usuarios_path %>
